<h1>Pirate</h1>
<div class="Pirate">
  <h2>Pirate Name: <%= @pirate.name %></h2><br>
  <h2>Pirate Weight: <%= @pirate.weight %></h2><br>
  <h2>Pirate Height: <%= @pirate.height %></h2><br>
</div><br>

<h1>Ships</h1>
<% @ships.each do |ship| %>
  <div class="ship">
  <h2>Name: <%= ship.name %></h2><br>
  <h2>Type: <%= ship.type %></h2><br>
  <h2>Booty: <%= ship.booty %></h2>
</div><br>
<% end %>
